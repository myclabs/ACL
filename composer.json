{
    "name": "myclabs/acl",
    "description": "ACL library for Doctrine",
    "keywords": ["acl", "doctrine", "permission"],
    "homepage": "https://github.com/myclabs/ACL",
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "MyCLabs\\ACL\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\MyCLabs\\ACL\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.5.0",
        "doctrine/orm": "2.5.*@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "4.1.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
